{
  "expo": {
    "name": "Campus Life",
    "slug": "campus-life",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "campuslife",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#111827"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.campuslife.app",
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "venmo",
          "paypal",
          "cashapp"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#111827"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.campuslife.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "campuslife"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8c66d3a1-b6ce-49e3-a654-83c1de4f9193"
      }
    },
    "owner": "ronaldli08"
  }
}
